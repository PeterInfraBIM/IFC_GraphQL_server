
type Ref {
	type: String!
	ref: String!
}

type Root implements Addressable & IfcRoot {
	type: String!
	globalId: ID!
	name: String
	description: String
	ownerHistoryRef: Ref
	ownerHistory: OwnerHistory
}

type OwnerHistory implements Addressable {
	type: String!
	globalId: ID!
