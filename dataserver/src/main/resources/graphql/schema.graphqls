}

type Organization {
	type: String!
	name: String
	description: String
}

type Application {
	type: String!
	applicationDeveloper: Organization
	version: String
	applicationFullName: String
	applicationIdentifier: String
}

type ObjectDefinition implements Addressable & IfcRoot & IfcObjectDefinition {
	type: String!
